{
  "name": "RaceMMO",
  "version": "0.0.0",
  "description": "A Racing MMO Server",
  "author": "Matt Lyons <matt@matt-lyons.net",
  "private": true,
  "scripts": {
    "build": "node ./node_modules/browserify/bin/cmd.js client/gameClient.js -o public/javascripts/game.js",
    "buildDebug": "node ./node_modules/browserify/bin/cmd.js client/gameClient.js -o public/javascripts/game.js -d; echo Build Complete!",
    "start": "node ./bin/www.js",
    "test": "node ./node_modules/mocha/bin/mocha ./test",
    "coverage": "node ./node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec",
    "loadTest": "node ./test/stressTest.js"
  },
  "dependencies": {
    "body-parser": "~1.14.1",
    "charm": "~1.0.0",
    "cookie-parser": "~1.4.0",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "node-uuid": "~1.4.3",
    "socket.io": "~1.3.5",
    "browserify": "~11.2.0"
  },
  "devDependencies": {
    "istanbul": "~0.3.17",
    "mocha": "~2.3.3",
    "nodemon": "~1.7.1",
    "should": "~7.1.0",
    "socket.io-client": "~1.3.5",
    "watchify": "~3.4.0"
  }
}
