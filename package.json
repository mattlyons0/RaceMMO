{
  "name": "RaceMMO",
  "version": "0.0.0",
  "description": "A Racing MMO Server",
  "author": "Matt Lyons <matt@matt-lyons.net>",
  "private": true,
  "scripts": {
    "build": "node ./node_modules/browserify/bin/cmd.js client/gameClient.js -o public/javascripts/game.js -d",
    "watch": "node ./node_modules/watchify/bin/cmd.js client/gameClient.js -o public/javascripts/game.js -dv",
    "start": "node ./bin/www.js",
    "devRun": "sh ./devRun.sh",
    "test": "node ./node_modules/mocha/bin/mocha ./test",
    "coverage": "node ./node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec",
    "loadTest": "node ./test/stressTest.js"
  },
  "dependencies": {
    "body-parser": "~1.14.1",
    "charm": "~1.0.0",
    "cookie-parser": "~1.4.0",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "uuid": "~2.0.1",
    "socket.io": "~1.3.5",
    "socket.io-client": "~1.3.5",
    "browserify": "~12.0.0",
    "deep-diff": "~0.3.3",
    "deepcopy": "~0.5.0",
    "toobusy-js": "~0.4.2"
  },
  "devDependencies": {
    "istanbul": "~0.4.0",
    "mocha": "~2.3.3",
    "nodemon": "~1.8.0",
    "should": "~7.1.1",
    "onchange": "~2.0.0"
  },
  "browser": {
    "./server/serverCore.js": false,
    "app.js": false,
    "./bin/www.js": false,
    "./server/commandLine.js": false,
    "./server/gameServer.js": false
  }
}
